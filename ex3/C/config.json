{
  "seed": 1,
  "training_data": "../../training_data/mnist_train.csv",
  "selection_method": {
    "total_size": 1000,
    "type": "simple_selection",

  },


  "learning_constant": 0.5,
  "limit": 100,
  "epsilon": 300,
  "batch_size": 1,

  "hidden_layer_amount": 2,
  "neurons_per_layer": 16,

  "activation_function": {
    "type": "logistic_function",
    "beta": 1
  },
  "optimization_method": {
    "type": "momentum",
    "alpha": 0.5
  }
}